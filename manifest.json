{
  "manifest_version": 2,
  "name": "Twitter Unreliable Badge",
  "author": "sharapeco",
  "version": "1.0.1",
  "description": "Adds badge to unreliable account name and header.",
  "icons": {
    "48": "icon-48.png"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "css": ["style.css"]
    }
  ],
  "permissions": [
    "*://twitter.com/*"
  ]
}
